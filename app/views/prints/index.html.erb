<h1>Listing prints</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Swatch uid</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @prints.each do |print| %>
  <tr>
    <td><%= print.name %></td>
    <td><%= print.swatch_uid %></td>
    <td><%= link_to 'Show', print %></td>
    <td><%= link_to 'Edit', edit_print_path(print) %></td>
    <td><%= link_to 'Destroy', print, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Print', new_print_path %>
